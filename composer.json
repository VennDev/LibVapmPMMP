{
    "name": "vennv/vapm-pmmp",
    "keywords": ["vapm-pmmp", "asynchronous", "LibVapmPMMP", "lib-vapm-pmmp", "process", "thread"],
    "license": "LGPL-2.0-only",
    "version": "2.5.4",
    "autoload": {
        "psr-4": {
            "vennv\\vapm\\": "src/vennv/vapm/"
        },
        "files": [
            "src/vennv/vapm/io/functions.php",
            "src/vennv/vapm/ct/functions.php"
        ]
    },
    "authors": [
        {
            "name": "VennDev",
            "email": "1050080275@sv.hcmunre.edu.vn"
        }
    ],
    "config": {
    	"allow-plugins": {
    		"phpstan/extension-installer": true
    	}
    },
    "require": {
        "php": ">=8.2",
    	"php-64bit": "*",
    	"composer-runtime-api": "^2.0",
        "pocketmine/pocketmine-mp": "^5.17.0"
    },
    "require-dev": {
        "phpstan/phpstan": "*"
    },
    "scripts": {
        "analyse-src": "./vendor/bin/phpstan analyse -c phpstan.neon.dist"
    },
    "extra": {
        "virion": {
            "spec": "3.0",
            "namespace-root": "vennv\\vapm"
        }
    }
}
